        System.out.println("Ingrese la dimencion fibonacci: ");
        n = leer.nextInt();

        for (i = 0; i < n; i++) {
            vector[i] = a;
            System.out.println(+vector[i]);

            c = a + b;
            a = b;
            b = c;
