    for(i=0;i<n;i++){
    System.out.println("ingresa un numero");
    vector[i]=leer.nextInt();
    
    }
    for(i=0;i<=(n-1);i++){
      for(j=0;j<(n-1);j++){
          if(vector[j]>vector[j+1]){
              aux=vector[j];
              vector[j]=vector[j+1];
              vector[j+1]=aux;
          }
      }  
    }
    for(i=0;i<n;i++){
        System.out.println("");
        System.out.println(vector[i]+" - ");
         }
         
